# PSRC Data Portal Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
PSRC_DB_SERVER=your_sql_server_name
PSRC_DB_NAME=your_database_name

# Portal Configuration
PSRC_PORTAL_USERNAME=your_portal_username
PSRC_PORTAL_PASSWORD=your_portal_password
PSRC_PORTAL_URL=https://psregcncl.maps.arcgis.com

# Optional Configuration Overrides
# Uncomment and modify as needed:

# Database Settings
# PSRC_DB_DRIVER=ODBC Driver 17 for SQL Server
# PSRC_DB_TIMEOUT=30

# Portal Settings
# PSRC_PORTAL_TIMEOUT=60

# Path Settings
# PSRC_WORKSPACE_PATH=./workspace
# PSRC_SDE_FOLDER=./sde
# PSRC_SDE_NAME=elmergeo.sde

# Spatial Settings
# PSRC_DEFAULT_SRID=2285
# PSRC_SDE_INSTANCE=SQLserver
# PSRC_SDE_DATABASE=ElmerGeo

# Logging Settings
# PSRC_LOG_LEVEL=INFO
# PSRC_LOG_FILE=  # Leave empty for console logging only
